{
  "display": "@rageatm/tsconfig/common",
  "extends": "@tsconfig/node-lts/tsconfig.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "declaration": true,
    "newLine": "lf",
    "outDir": "../../../dist",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2022"],
    "incremental": true
  }
}
